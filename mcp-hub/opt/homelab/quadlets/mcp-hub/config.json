{
	"servers": {
		"context7": {
			"disabled": false,
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp",
				"--api-key",
				"${CONTEXT7_API_KEY}"
			],
			"env": {}
		},
		"mcpadvisor": {
			"disabled": false,
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@xiaohui-wang/mcpadvisor"
			],
			"env": {}
		}
	},
	"auth": {
		"type": "none"
	},
	"cors": {
		"enabled": true,
		"origin": "*"
	}
}
